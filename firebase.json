{
    "hosting": {
        "site": "trail-lab-website",
        "public": "out",
        "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
        "redirects": [
            {
                "source": "/aibat",
                "destination": "https://aibat-frontend-1008901154916.us-central1.run.app",
                "type": 302
            },
            {
                "source": "/aibat/**",
                "destination": "https://aibat-frontend-1008901154916.us-central1.run.app",
                "type": 302
            },
            {
                "source": "/bias-audit",
                "destination": "https://trail-lab-dev.web.app",
                "type": 302
            },
            {
                "source": "/bias-audit/**",
                "destination": "https://trail-lab-dev.web.app",
                "type": 302
            }
        ],
        "rewrites": [
            {
                "source": "/research",
                "destination": "/research.html"
            },
            {
                "source": "/research/**",
                "destination": "/research.html"
            },
            {
                "source": "/publications",
                "destination": "/publications.html"
            },
            {
                "source": "/publications/**",
                "destination": "/publications.html"
            },
            {
                "source": "/resources",
                "destination": "/resources.html"
            },
            {
                "source": "/resources/**",
                "destination": "/resources.html"
            },
            {
                "source": "/people",
                "destination": "/people.html"
            },
            {
                "source": "/people/**",
                "destination": "/people.html"
            },
            {
                "source": "/news",
                "destination": "/news.html"
            },
            {
                "source": "/news/**",
                "destination": "/news.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}
